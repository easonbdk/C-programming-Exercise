// Use character pointer to print strings. (One dimensional matrix)

#include <stdio.h>

int main(void) {
    char *str[] = {"professor", "teacher", "student", "etc."};

    for(int i = 0; i < 4; i++) {
        puts(str[i]);
    }

    return 0;
}
